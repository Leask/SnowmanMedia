        </SkinObject>

        <SkinObject
          align=client
          kind = "bottom"
          BackgroundRect = "web.bmp" 77 44 87 67
          OpacityMapRect = "web-mask.bmp" 77 44 87 67          
          >
        </SkinObject>
    </SkinObject>

    <SkinObject 
      align=left 
      kind = "left"
      BackgroundRect = "web.bmp" 0 27 4 37
      >
    </SkinObject>

    <SkinObject 
      align=right
      kind = "right"
      BackgroundRect = "web.bmp" 290 27 294 37
      >
    </SkinObject>

    <SkinObject CLASS = "client"
      align=client
      kind = "client"
      BackgroundRect = "web.bmp" 15 27 25 37
      >
    </SkinObject>

</SkinObject>


