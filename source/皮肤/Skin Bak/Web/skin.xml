<SkinObject CLASS="form"
>
    <SkinObject CLASS="top_edge"
      align=top
      height=26
    >

        <SkinObject CLASS="for_menu"
          align=left
          BackgroundRect = "web.bmp" 0 0 24 26
          OpacityMapRect = "web-mask.bmp" 0 0 24 26
          >
        </SkinObject>

        <SkinObject
          left = 8 top = 2
          kind=sysmenu
          BackgroundRect = "web.bmp" 8 2 23 20
          OnMouseIn = "stop object ; MorphToRect web-active.bmp 8 2 23 20 300"
          OnMouseOut = "stop object ; MorphToRect web.bmp 8 2 23 20 1000"
        >
        </SkinObject>

        <SkinObject
          align=right
          BackgroundRect = "web.bmp" 286 0 294 26
        >
        </SkinObject>

        <SkinObject
          align=client
          kind = "title"
          color=13735955
          BackgroundRect = "web.bmp" 33 0 43 26
          >
