    </SkinObject>


    <SkinObject CLASS="bottom_edge"
      align=bottom
      height=23
    >
        <SkinObject 
          align=left
          kind = "bottomleft"
          BackgroundRect = "web.bmp" 0 44 14 67
          OpacityMapRect = "web-mask.bmp" 0 44 14 67
          >
        </SkinObject>

        <SkinObject 
          align=right
          kind = "bottomright"
          BackgroundRect = "web.bmp" 154 44 294 67
          OpacityMapRect = "web-mask.bmp" 154 44 294 67          
