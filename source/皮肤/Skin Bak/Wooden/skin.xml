          OnMouseIn = "stop object ; MorphToRect Woodie-active.bmp 297 4 316 34 500"
          OnMouseOut = "stop object ; MorphToRect Woodie.bmp 297 4 316 34 1000"
          >
        </SkinObject>

        <SkinObject
          align=bottom
          kind=caption
          BackgroundRect = "Woodie.bmp" 84 32 104 34
          >
        </SkinObject>

        <SkinObject
          align=client
          kind=title color=65380
          BackgroundRect = "Woodie.bmp" 84 4 104 32
          >
        </SkinObject>

    </SkinObject>

    <SkinObject CLASS="bottom_edge"
