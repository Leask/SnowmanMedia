<SkinObject link="control" CLASS="TPanel" > </SkinObject>
<SkinObject link="control" CLASS="ThunderTextBox" > </SkinObject>
<SkinObject link="control" CLASS="ThunderRT5TextBox" > </SkinObject>
<SkinObject link="control" CLASS="ThunderCommandButton" > </SkinObject>
<SkinObject link="control" CLASS="ThunderRT5CommandButton" > </SkinObject>
<SkinObject link="control" CLASS="ThunderCheckBox" > </SkinObject>
<SkinObject link="control" CLASS="ThunderRT5CheckBox" > </SkinObject>

