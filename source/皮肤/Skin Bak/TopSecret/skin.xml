<SkinObject CLASS="control"
  OnMouseIn = "goto top_left OnMyActivate"
  OnMouseOut = "goto top_left OnMyDeactivate"
>

    <SkinObject CLASS="top_left"
      left = 0 top = 0 
      kind = "caption"
      backgroundRect = "TopSecret.bmp" 164 28 167 31
      OnMyActivate = "stop object ; MorphToRect TopSecret-active.bmp 164 28 167 31 500 ; goto top_line OnMyActivate"
      OnMyDeactivate = "stop object ; MorphToRect TopSecret.bmp 164 28 167 31 1000 ; goto top_line OnMyDeactivate"
      >
    </SkinObject>

    <SkinObject CLASS="top_line"
      left = 3 right = 100% top = 0 backalign=stretch
      kind = "caption"
      backgroundRect = "TopSecret.bmp" 167 28 201 31
      OnMyActivate = "stop object ; MorphToRect TopSecret-active.bmp 167 28 201 31 500 ; goto left_line OnMyActivate"
      OnMyDeactivate = "stop object ; MorphToRect TopSecret.bmp 167 28 201 31 1000 ; goto left_line OnMyDeactivate"
      >
    </SkinObject>

    <SkinObject CLASS="left_line" 
      left = 0 top = 3 bottom = 100% backalign=stretch
      kind = "left"
      backgroundRect = "TopSecret.bmp" 164 31 167 67
      OnMyActivate = "stop object ; MorphToRect TopSecret-active.bmp 164 31 167 67 500 ; goto right_line OnMyActivate"
      OnMyDeactivate = "stop object ; MorphToRect TopSecret.bmp 164 31 167 67 1000 ; goto right_line OnMyDeactivate"
      >
    </SkinObject>

    <SkinObject CLASS="right_line" 
      right = 100% top = 0 bottom = 100%-3 backalign=stretch
      kind = "right"
      backgroundRect = "TopSecret.bmp" 198 28 201 64
      OnMyActivate = "stop object ; MorphToRect TopSecret-active.bmp 198 28 201 64 500 ; goto bottom_line OnMyActivate"
      OnMyDeactivate = "stop object ; MorphToRect TopSecret.bmp 198 28 201 64 1000 ; goto bottom_line OnMyDeactivate"
      >
    </SkinObject>

    <SkinObject CLASS="bottom_line" 
      left = 0 bottom = 100% right = 100%-3 backalign=stretch
      kind = "bottom"
      backgroundRect = "TopSecret.bmp" 164 64 198 67
      OnMyActivate = "stop object ; MorphToRect TopSecret-active.bmp 164 64 198 67 500 ; goto bottom_right OnMyActivate"
      OnMyDeactivate = "stop object ; MorphToRect TopSecret.bmp 164 64 198 67 1000 ; goto bottom_right OnMyDeactivate"
      >
    </SkinObject>

    <SkinObject CLASS="bottom_right" 
      right = 100% bottom = 100% 
      kind = "bottomright"
      backgroundRect = "TopSecret.bmp" 198 64 201 67
      OnMyActivate = "stop object ; MorphToRect TopSecret-active.bmp 198 64 201 67 500 "
      OnMyDeactivate = "stop object ; MorphToRect TopSecret.bmp 198 64 201 67 1000 "
      >
    </SkinObject>

    <SkinObject CLASS="client"
      kind=client
      left = 3
      top = 3
      right = 100%-3
      bottom = 100%-3
      opacity=255
    >
    </SkinObject>

</SkinObject>

