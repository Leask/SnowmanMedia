title = "Custom Skin Demo"
artist = "Sergey Belous, SoftShape Development"

<SkinObject CLASS="form"
 kind=caption
>
    <SkinObject CLASS="body"
       left=0 top=0
       Background="helen.bmp"
       opacitymap="helen-mask.bmp"
    >
	</SkinObject>

    <SkinObject CLASS="logo"
       kind = border
       left=17 top=43
       BackgroundRect="helen.bmp" 17 43 161 121
       OnLButtonDown = "fireevent OnClickLogo"
       OnMouseIn = "stop object ; MorphToRect helen-active.bmp 17 43 161 121 3000 ;
         spawn A OnMouseIn ; wait 150 ;
         spawn A OnMouseOut ; spawn B OnMouseIn ; wait 150 ;
         spawn B OnMouseOut ; spawn C OnMouseIn ; wait 150 ;
         spawn C OnMouseOut ; spawn D OnMouseIn ; wait 150 ;
         spawn D OnMouseOut"
       OnMouseOut = "
         spawn A OnMouseOut ; spawn B OnMouseOut ;
         spawn C OnMouseOut ; spawn D OnMouseOut ;
         stop object ; MorphToRect helen.bmp  17 43 161 121 3000"
    >
	</SkinObject>

    <SkinObject CLASS="A"
       kind=border
       left=99 top=151
       BackgroundRect="helen.bmp" 99 151 119 171
       OnLButtonDown = "fireevent OnClickA"
       OnMouseIn = "stop object ; MorphToRect helen-active.bmp 99 151 119 171 500"
       OnMouseOut = "stop object ; MorphToRect helen.bmp  99 151 119 171 1000"
    >
	</SkinObject>

    <SkinObject CLASS="B"
       kind=border
       left=79 top=157
       BackgroundRect="helen.bmp" 79 157 97 169
       OnLButtonDown = "fireevent OnClickB"
       OnMouseIn = "stop object ; MorphToRect helen-active.bmp 79 157 97 169 500"
       OnMouseOut = "stop object ; MorphToRect helen.bmp  79 157 97 169 1000"
    >
	</SkinObject>

    <SkinObject CLASS="C"
       kind=border
       left=5 top=67
       BackgroundRect="helen.bmp" 5 67 17 107
       OnLButtonDown = "fireevent OnClickC"
       OnMouseIn = "stop object ; MorphToRect helen-active.bmp 5 67 17 107 500"
       OnMouseOut = "stop object ; MorphToRect helen.bmp  5 67 17 107 1000"
    >
	</SkinObject>

    <SkinObject CLASS="D"
       kind=border
       left=161 top=61
       BackgroundRect="helen.bmp" 161 61 171 101
       OnLButtonDown = "fireevent OnClickD"
       OnMouseIn = "stop object ; MorphToRect helen-active.bmp 161 61 171 101 500"
       OnMouseOut = "stop object ; MorphToRect helen.bmp 161 61 171 101 1000"
    >
	</SkinObject>

    <SkinObject CLASS="close"
       left=57 top=157
       kind = close
       BackgroundRect="helen.bmp" 57 157 77 161
       OnMouseIn = "stop object ; MorphToRect helen-active.bmp 57 157 77 161 500"
       OnMouseOut = "stop object ; MorphToRect helen.bmp 57 157 77 161 1000"
    >
	</SkinObject>


</SkinObject>

