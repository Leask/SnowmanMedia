title = "Custom Skin Demo"
artist = "Sergey Belous, SoftShape Development"

<SkinObject CLASS="form"
 kind=caption
>
    <SkinObject CLASS="body"
       left=0 top=0
       Background="Greate.bmp"
       opacitymap="Greate-mask.bmp"
    >
	</SkinObject>

    <SkinObject CLASS="logo"
       kind = border
       left=26 top=3
       BackgroundRect="Greate.bmp" 26 3 49 10
       OnLButtonDown = "fireevent OnClickLogo"
       OnMouseIn = "stop object ; MorphToRect Greate-active.bmp 26 3 49 10 3000 ;
         spawn A OnMouseIn ; wait 150 ;
         spawn A OnMouseOut ; spawn B OnMouseIn ; wait 150 ;
         spawn B OnMouseOut ; spawn C OnMouseIn ; wait 150 ;
         spawn C OnMouseOut ; spawn F OnMouseIn ; wait 150 ;
         spawn F OnMouseOut ; spawn H OnMouseIn ; wait 150 ;
         spawn H OnMouseOut"
       OnMouseOut = "
         spawn A OnMouseOut ; spawn B OnMouseOut ;
         spawn C OnMouseOut ; spawn F OnMouseOut ; spawn H OnMouseOut ;
         stop object ; MorphToRect Greate.bmp  26 3 49 10 3000"
    >
	</SkinObject>

    <SkinObject CLASS="A"
       kind=border
       left=72 top=3
       BackgroundRect="Greate.bmp" 72 3 97 10
       OnLButtonDown = "fireevent OnClickA"
       OnMouseIn = "stop object ; MorphToRect Greate-active.bmp 72 3 97 10 500"
       OnMouseOut = "stop object ; MorphToRect Greate.bmp  72 3 97 10 1000"
    >
	</SkinObject>

    <SkinObject CLASS="B"
       kind=border
       left=50 top=3
       BackgroundRect="Greate.bmp" 50 3 73 10
       OnLButtonDown = "fireevent OnClickB"
       OnMouseIn = "stop object ; MorphToRect Greate-active.bmp 50 3 73 10 500"
       OnMouseOut = "stop object ; MorphToRect Greate.bmp  50 3 73 10 1000"
    >
	</SkinObject>

    <SkinObject CLASS="C"
       kind=border
       left=26 top=126
       BackgroundRect="Greate.bmp" 26 126 49 134
       OnLButtonDown = "fireevent OnClickC"
       OnMouseIn = "stop object ; MorphToRect Greate-active.bmp 26 126 49 134 500"
       OnMouseOut = "stop object ; MorphToRect Greate.bmp  26 126 49 134 1000"
    >
	</SkinObject>

    <SkinObject CLASS="F"
       kind=border
       left=49 top=126
       BackgroundRect="Greate.bmp" 49 126 73 134
       OnLButtonDown = "fireevent OnClickD"
       OnMouseIn = "stop object ; MorphToRect Greate-active.bmp 49 126 73 134 500"
       OnMouseOut = "stop object ; MorphToRect Greate.bmp 49 126 73 134 1000"
    >
	</SkinObject>

   <SkinObject CLASS="H"
       kind=border
       left=73 top=126
       BackgroundRect="Greate.bmp" 49 126 73 134
       OnLButtonDown = "fireevent OnClickD"
       OnMouseIn = "stop object ; MorphToRect Greate-active.bmp 49 126 73 134 500"
       OnMouseOut = "stop object ; MorphToRect Greate.bmp 49 126 73 134 1000"
    >
	</SkinObject>




    <SkinObject CLASS="close"
       left=9 top=120
       BackgroundRect="Greate.bmp" 9 120 18 125
       OnMouseIn = "stop object ; MorphToRect Greate-active.bmp 9 120 18 125 500"
       OnMouseOut = "stop object ; MorphToRect Greate.bmp 9 120 18 125 1000"
    >
	</SkinObject>


</SkinObject>

