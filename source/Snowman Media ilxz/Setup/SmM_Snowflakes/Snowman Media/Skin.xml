title  = "Snowman Media"
artist = "Leask"
info   = "这是第一个 Snowflake!"

<SkinObject CLASS="form"
 kind=caption
>
    <SkinObject CLASS="body"
       left=0 top=0
       Background="Snowman_Media.bmp"
       opacitymap="Snowman_Media-mask.bmp"
    >
	</SkinObject>

    <SkinObject CLASS="all"
       kind = border
       left=72 top=112
       BackgroundRect="Snowman_Media.bmp" 72 112 120 130
       OnLButtonDown = "fireevent OnClickLogo"
       OnMouseIn = "stop object ; MorphToRect Snowman_Media-active.bmp 72 112 120 130 3000 ;
         spawn A OnMouseIn ; wait 150 ;
         spawn A OnMouseOut ; spawn B OnMouseIn ; wait 150 ;
         spawn B OnMouseOut ; spawn C OnMouseIn ; wait 150 ;
         spawn C OnMouseOut ; spawn F OnMouseIn ; wait 150 ;
         spawn F OnMouseOut"
       OnMouseOut = "
         spawn A OnMouseOut ; spawn B OnMouseOut ;
         spawn C OnMouseOut ; spawn F OnMouseOut ;
         stop object ; MorphToRect Snowman_Media.bmp  72 112 120 130 3000"
    >
	</SkinObject>





    <SkinObject CLASS="A"
       kind=border
       left=13 top=89
       BackgroundRect="Snowman_Media.bmp" 13 89 37 105
       OnLButtonDown = "fireevent OnClickA"
